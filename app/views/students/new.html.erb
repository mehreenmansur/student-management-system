<%= form_for :student, url: students_path do |f| %>
    <p>
      <%= f.label :name %><br>
      <%= f.text_field :name %>
    </p>

    <p>
      <%= f.label :id %><br>
      <%= f.text_field :id %>
    </p>
    <p>
      <%= f.label :image %><br>
      <%= f.file_field :image %>
    </p>

    <p>
      <%= f.submit %>
    </p>
<% end %>